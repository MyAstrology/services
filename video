  <script>
    // Video card interaction
    document.querySelectorAll('.video-card').forEach(card => {
      card.addEventListener('click', (e) => {
        // Only open video if not clicking on a button or link
        if (!e.target.closest('.watch-button') && !e.target.closest('.play-icon')) {
          const link = card.querySelector('.watch-button');
          if (link) {
            window.open(link.href, '_blank');
          }
        }
      });
    });
    
    // Category filtering
    document.querySelectorAll('.category-btn').forEach(btn => {
      btn.addEventListener('click', () => {
        // Remove active class from all buttons
        document.querySelectorAll('.category-btn').forEach(b => {
          b.classList.remove('active');
        });
        
        // Add active class to clicked button
        btn.classList.add('active');
        
        // In a real implementation, you would filter videos here
      });
    });
  </script>
</body>
